{
  "detectors_to_exclude": [
    "naming-convention",
    "external-function",
    "low-level-calls"
  ],
  "filter_paths": [
    "lib/",
    "node_modules/",
    "test/"
  ],
  "solc_remaps": [
    "ds-test/=lib/forge-std/lib/ds-test/src/",
    "forge-std/=lib/forge-std/src/",
    "@prb/math/=lib/prb-math/",
    "@openzeppelin/=lib/openzeppelin-contracts/",
    "@chainlink/contracts=lib/chainlink/contracts/"
  ]
}

